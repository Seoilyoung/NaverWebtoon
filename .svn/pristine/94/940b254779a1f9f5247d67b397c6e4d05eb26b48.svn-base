package gabriel.com.nwebtoon_android.model;

import com.orm.SugarRecord;

public class LocalData extends SugarRecord{
    boolean Rotation ;
    boolean Network_Notification ;
    boolean Auto_Scroll ;
    public String naver_id;

    public LocalData() {
    }

    public LocalData(boolean Rotation, boolean Network_Notification, boolean Auto_Scroll) {
        this.Rotation = Rotation ;
        this.Network_Notification = Network_Notification ;
        this.Auto_Scroll = Auto_Scroll ;
    }

    public boolean getRotation() {
        return this.Rotation ;
    }
    public void setRotation(boolean Rotation) {
        this.Rotation = Rotation ;
    }
    public boolean getNetwork() {
        return this.Network_Notification ;
    }
    public void setNetwork(boolean Network_Notification) {
        this.Network_Notification = Network_Notification ;
    }
    public boolean getScroll() {
        return this.Auto_Scroll ;
    }
    public void setScroll(boolean Auto_Scroll) {
        this.Auto_Scroll = Auto_Scroll ;
    }
    public String getnaver_id() {
        return this.naver_id ;
    }
    public void setnaver_id(String naver_id) {
        this.naver_id = naver_id ;
    }
    public void initnaver_id(){
        this.naver_id=null;
    }
}
