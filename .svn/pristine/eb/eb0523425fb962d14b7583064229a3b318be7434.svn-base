<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="160dp">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">
    <RelativeLayout
        android:id="@+id/webtoon_item1"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1">
        <ImageView
            android:id="@+id/img_thumbnail1"
            android:layout_alignParentTop="true"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:scaleType="fitXY"/>
        <TextView
            android:id="@+id/txt_list_name1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            android:layout_below="@id/img_thumbnail1"
            android:layout_alignParentLeft="true"
            android:text="웹툰 제목"
            android:textSize="13sp"
            android:textStyle="bold"
            android:textColor="#000000" />
        <ImageView
            android:id="@+id/star_icon1"
            android:layout_width="15dp"
            android:layout_height="17dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="5dp"
            android:layout_below="@id/txt_list_name1"
            android:layout_alignParentLeft="true"
            android:scaleType="fitXY"
            android:background="@drawable/ic_star_border_red_24dp"/>
        <TextView
            android:id="@+id/txt_rating1"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/txt_list_name1"
            android:layout_toRightOf="@id/star_icon1"
            android:text="9.98"
            android:textSize="13sp"
            android:textColor="#ff0000"
            />
        <ImageView
            android:id="@+id/status_icon1"
            android:layout_width="45dp"
            android:layout_height="20dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="5dp"
            android:layout_below="@id/txt_list_name1"
            android:layout_toRightOf="@id/txt_rating1"
            android:scaleType="fitXY" />
        <TextView
            android:id="@+id/txt_author1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="5dp"
            android:text="작가명"
            android:textSize="13sp"
            android:layout_alignParentLeft="true"
            android:layout_below="@id/txt_rating1"/>
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#a9a9a9"
            android:layout_alignParentRight="true" />
    </RelativeLayout>

